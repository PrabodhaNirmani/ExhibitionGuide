                    android:layout_weight=".06"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:id="@+id/route_up"
                    android:orientation="horizontal"
                    android:layout_height="0dp">
                    <ImageView
